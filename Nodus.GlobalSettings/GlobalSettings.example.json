    },
    "AdminDatabase": ""
  },

  "Auth": {
    "AuthApiUrl": "http://nodus.auth",
    "PublicKey": ""
  },

  "BotOptions": {
    "NodusApiURI": "http://localhost:3000"
  },

  "AllowedHosts": "*"
}
